{
  "main": "server.js",
  "name": "momentous",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pdxbellasaurus/momentousReact.git"
  },
  "dependencies": {
    "@geoapify/geocoder-autocomplete": "^1.3.2",
    "@geoapify/react-geocoder-autocomplete": "^1.3.1",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "connect-mongodb-session": "^2.4.1",
    "express": "^4.16.3",
    "express-session": "^1.17.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "react": "^17.0.2",
    "react-datetime-picker": "^3.3.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "homepage": "https://github.com/pdxbellasaurus/momentousReact#readme",
